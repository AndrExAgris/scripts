# Simple
# alias atalho="comando"
alias update="sudo apt update"
alias upgrade="sudo apt upgrade -y"
alias install="sudo apt install -y"
alias dinoinfo='neofetch --ascii "$(cowsay -f stegosaurus DinoInfo ao seu dispor meu fino senÃµr)" | lolcat'
alias backup="cd && ./Documents/backup/backup.sh"
